<h1>All products</h1>
<div class="products">
    {{#each products}}
    <div class="product">
        <h3>{{this.name}}</h3>
        <img src="{{this.image}}" alt="">
        <p>R$ {{this.price}} </p>
        <div class="actions">
            <a href="/products/{{this._id}}">Detalhes</a>
            <a href="/products/edit/{{this._id}}">Editar</a>
        </div>
    </div>
    {{/each}}
</div>